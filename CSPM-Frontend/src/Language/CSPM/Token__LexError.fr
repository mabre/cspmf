-- This "forward declares" LexError so that the make script can compile the java dependencies.
module Language.CSPM.Token__LexError
where

import Data.Data

{-# derive DataTypeable #-}
data LexError = ! LexError {
   lexEPos :: AlexPosn
  ,lexEMsg :: String
  }
derive Show LexError

{-# derive DataTypeable #-}
data AlexPosn = ! AlexPn {
   alexPos :: Int
  ,alexLine   :: Int 
  ,alexCol    :: Int
  }
derive Eq AlexPosn
derive Ord AlexPosn
derive Show AlexPosn
